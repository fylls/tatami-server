{
	"name": "tatami-server",
	"version": "1.0.0",
	"description": "I hate gamers",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon src/app.ts",
		"start": "node dist/app.js",
		"build": "tsc -p .",
		"delete": "rm -rf node_modules/",
		"update": "rm -rf node_modules/ && npm install -g npm-check-updates && ncu -u && npm install",
		"zip": "tsc -p . && cd .. && zip -r tatami-server/output.zip tatami-server -x \"tatami-server/src/*\" \"tatami-server/node_modules/*\" \"tatami-server/resources/*\""
	},
	"author": "philly joe",
	"license": "WTFPL",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"mongoose": "^6.3.3",
		"stripe": "^9.1.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.34",
		"@types/stripe": "^8.0.417",
		"nodemon": "^2.0.16",
		"ts-node": "^10.7.0",
		"tslint": "^6.1.3"
	},
	"packageManager": "yarn@3.2.0"
}
